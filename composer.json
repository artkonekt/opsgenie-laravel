{
    "name": "konekt/opsgenie-laravel",
    "description": "OpsGenie Notifications Channel for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": ["notifications", "notification-channel", "alerting", "opsgenie", "konekt", "artkonekt", "laravel"],
    "support": {
        "source": "https://github.com/artkonekt/opsgenie-laravel"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/notifications": "^8.22.1|^9.0",
        "illuminate/support": "^8.22.1|^9.0",
        "illuminate/http": "^8.22.1|^9.0",
        "guzzlehttp/guzzle": "^6.5.5|^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": { "Konekt\\OpsGenie\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\OpsGenie\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Konekt\\OpsGenie\\OpsGenieServiceProvider"
            ]
        }
    }
}
